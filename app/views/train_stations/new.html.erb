<h1>New train_station</h1>

<% form_for(@train_station) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :crs_code %><br />
    <%= f.text_field :crs_code %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :longitude %><br />
    <%= f.text_field :longitude %>
  </p>
  <p>
    <%= f.label :latitude %><br />
    <%= f.text_field :latitude %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', train_stations_path %>